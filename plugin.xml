<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.imp.runtime.languageDescription">
      <language
            extensions="jav"
            description="JavaLang"
            language="JavaLang"
            derivedFrom="DynamicRoot"
            validator="JavaLang.JavaLangValidator"
            validatorClass="JavaLang.JavaLangValidator"
      ></language>
   </extension>
   <extension
         id="JavaLang.parser"
         name="JavaLang Parser"
         point="org.eclipse.imp.runtime.parser">
      <parser
            class="JavaLang.JavaLangParseController"
            language="JavaLang">
      </parser>
   </extension>    
</plugin>
